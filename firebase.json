{
  "functions": {
    "source": "functions",
    "runtime": "nodejs16"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/interseccion",
        "function": "interseccion"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
